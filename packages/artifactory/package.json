{
  "name": "@crestproject/artifactory",
  "version": "0.0.59",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "build": "hardhat compile && node codegen && tsdx build --target node --format cjs"
  },
  "devDependencies": {
    "@crestproject/codegen": "^0.0.59",
    "eslint-plugin-prettier": "3.1.4",
    "hardhat": "^2.0.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "@typescript-eslint/parser": "^4.1.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
